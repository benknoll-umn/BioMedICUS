      <zeroLengthEmitted>false</zeroLengthEmitted>
    </propertyCasMapping>
    <propertyCasMapping>
      <annotationClassName>edu.umn.biomedicus.rtfuima.type.Underline</annotationClassName>
      <minimumValue>1</minimumValue>
      <propertyGroup>CharacterFormatting</propertyGroup>
      <propertyName>Underline</propertyName>
      <valueIncluded>false</valueIncluded>
      <zeroLengthEmitted>false</zeroLengthEmitted>
    </propertyCasMapping>
    <propertyCasMapping>
