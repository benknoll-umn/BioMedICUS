      <controlWord>row</controlWord>
    </controlWordCasMapping>
    <controlWordCasMapping>
      <annotationName>edu.umn.biomedicus.rtfuima.type.CellEnd</annotationName>
      <controlWord>cell</controlWord>
    </controlWordCasMapping>
    <controlWordCasMapping>
      <annotationName>edu.umn.biomedicus.rtfuima.type.NestRowEnd</annotationName>
      <controlWord>nestrow</controlWord>
    </controlWordCasMapping>
    <controlWordCasMapping>
      <annotationName>edu.umn.biomedicus.rtfuima.type.NestCellEnd</annotationName>
      <controlWord>nestcell</controlWord>
